{
  "name": "web-admin",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web --port 3001",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "preset": "jest-expo/web",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|react-native-paper|@kinde-oss/.*)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^react-native$": "react-native-web"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@expo/metro-runtime": "^6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@expo/webpack-config": "^19.0.1",
    "@kinde-oss/kinde-auth-react": "^5.10.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.5.0",
    "@react-native-community/slider": "^5.1.1",
    "@react-native-picker/picker": "^2.11.4",
    "@react-navigation/native": "^7.1.21",
    "@react-navigation/stack": "^7.6.7",
    "@reduxjs/toolkit": "^2.11.0",
    "axios": "^1.13.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.7",
    "expo": "~54.0.25",
    "expo-auth-session": "^7.0.9",
    "expo-av": "^16.0.7",
    "expo-document-picker": "^14.0.7",
    "expo-file-system": "^19.0.19",
    "expo-image-manipulator": "^14.0.7",
    "expo-image-picker": "^17.0.8",
    "expo-media-library": "^18.2.0",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "^14.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "^15.0.9",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-infinite-wheel-picker": "^1.0.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "^4.1.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.2",
    "react-native-wheel-pick": "^1.2.6",
    "react-native-wheely": "^0.6.0",
    "react-native-worklets": "^0.6.1",
    "react-redux": "^9.2.0"
  },
  "private": true,
  "devDependencies": {
    "@testing-library/react-native": "^13.3.3",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.13",
    "react-test-renderer": "^19.1.0"
  }
}
